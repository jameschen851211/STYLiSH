{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-plusplus": ["error", { "allowForLoopAfterthoughts": true }],
        "no-alert":"off"
    },
    "globals": {
        "TPDirect": true,
        "FB": true,
        "keyword": true
    }
}
